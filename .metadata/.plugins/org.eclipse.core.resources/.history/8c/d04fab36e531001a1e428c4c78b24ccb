package pack;

import java.nio.channels.ShutdownChannelGroupException;

abstract class Automobile1{
	 abstract String show();
}
 interface fly
 {
	abstract int  fly(int a);
 }
class Aeroplane extends Automobile implements fly{
	int flightNo;
String show()
{
	return "aeroplane";
}
	@Override
	public int fly(int a) {
		// TODO Auto-generated method stub
		return 100;
	}
}
interface runnable{
	abstract int run(int speed);
	}
class Train extends Automobile implements runnable{

	String show()
	{
		return "train";
	}
	@Override
	public int run(int speed) {
		// TODO Auto-generated method stub
		return 500;
	}
}
interface swimable
{
	 abstract String swim();
	
}
class ship extends Automobile implements swimable{

	String show()
	{
		return "swim";
	}
	@Override
	public String swim() {
		// TODO Auto-generated method stub
		return "can swim";
	}
}

public class exapmle {
	ship s=new ship();
	s.swim();
	

}


