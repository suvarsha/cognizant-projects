package thread;

class Thread1 extends Thread
{
	@Override
	public void run() {
		// TODO Auto-generated method stub
		super.run();
	for(int i=0;i<=10;i++)
	{
		System.out.print(i);
	}
}
}
public class Mythread {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
Thread1 t=new Thread1();
t.start();
try {
	Thread1.sleep(1000);
} catch (InterruptedException e) {
	// TODO Auto-generated catch block
	e.printStackTrace();
}
	}
}
